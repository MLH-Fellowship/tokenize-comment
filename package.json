{
  "name": "tokenize-comment",
  "description": "Uses snapdragon to tokenize a single JavaScript block comment into an object, with description, tags, and code example sections that can be passed to any other comment parsers for further parsing.",
  "version": "1.0.2",
  "homepage": "https://github.com/jonschlinkert/tokenize-comment",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/tokenize-comment",
  "bugs": {
    "url": "https://github.com/jonschlinkert/tokenize-comment/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "lib"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "array-each": "^1.0.1",
    "define-property": "^0.2.5",
    "extend-shallow": "^2.0.1",
    "snapdragon": "^0.11.0"
  },
  "devDependencies": {
    "extract-comments": "^0.10.1",
    "gulp-format-md": "^0.1.11",
    "mocha": "^3.2.0"
  },
  "keywords": [
    "code",
    "comment",
    "examples",
    "gfm",
    "indented",
    "javadoc",
    "javascript",
    "jsdoc",
    "parse",
    "tokenize"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "parse-comments",
        "snapdragon",
        "strip-comments"
      ]
    },
    "lint": {
      "reflinks": true
    }
  }
}
